<html lang="en">


<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="https://www.gstatic.com/firebasejs/8.2.3/firebase-app.js"></script>
    <script src="https://www.gstatic.com/firebasejs/8.2.3/firebase-app.js"></script>
    <script src="https://www.gstatic.com/firebasejs/8.2.3/firebase-auth.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <link rel="stylesheet" href="/fontawesome-free-5.15.1-web/css/all.css">
    <link rel="stylesheet" href="/stylesheet/TypeTilesstyles.css">
    <link rel="stylesheet" href="/stylesheet/accountinfo.css">
    <title>Document</title>
</head>

<body>
    <div class="top">
        <h1 class="title">Designer Tiles</h1>
        <div class="nav">

            <a href="#"><i class="fas fa-user" style="padding-right: 5px;"></i>Account Info</a>
            <a id="wishlistlink" onclick="locationchange()"><i class="fas fa-heart" style="padding-right: 5px;"></i>Wishlist</a>
            <a id="cartlink" onclick="locationchange()"><i class="fas fa-cart-plus" style="padding-right: 5px;"></i>Cart</a>
            <a href="/TypeTile"><i class="fas fa-home" style="padding-right: 5px;"></i>Home</a>

        </div>
    </div>

    <div class="account_con">

        <div class="profile_info">
            <button onclick="signout()">
                signout
            </button>
            <img id="profile" alt="Avatar" style="width: 150px; height: 150px;">
            <h3 id="username">
                <%=accountdata[0].first_name+" "+accountdata[0].last_name%>
            </h3>
            <h4 id="email"></h4>
            </h4>
            <h4 id="phone_no">
                <%=accountdata[0].phone_no%>
            </h4>
        </div>

        <h4 style="font-weight: bold;">Location:</h4>
        <p style="padding-left: 30px;">
            <%=accountdata[0].location%>
        </p>
        <h4 style="font-weight: bold;">City:</h4>
        <p style="padding-left: 30px;">
            <%=accountdata[0].city%>
        </p>
        <h4 style="font-weight: bold;">Landmark:</h4>
        <p style="padding-left: 30px;">
            <%=accountdata[0].landmark%>
        </p>


    </div>
    <div class="container" style="position: fixed;
    top: 590px;
    left: 1250px;" onclick="myorders()">
        <div class="fixed-action-bt">
            <a class="btn-floating red">
                <i style="font-size: 25px; color: white;" class="fas fa-shopping-bag"></i>
            </a>

        </div>


    </div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>

</body>
<script src="/js/Login.js"></script>
<script script src="/js/accountinfo.js">
</script>

</html>