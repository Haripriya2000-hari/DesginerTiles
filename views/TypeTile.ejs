<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <scrip src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></scrip>
    <!-- The core Firebase JS SDK is always required and must be listed first -->
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>
    <!-- <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script> -->
    <link rel="stylesheet" href="fontawesome-free-5.15.1-web/css/all.css">
    <link rel="stylesheet" href="stylesheet/TypeTilesstyles.css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">


</head>

<body class="helo" onclick="close()">

    <div class="top">
        <h1 class="title">Designer Tiles</h1>
        <div class="container2">
            <input class="searchInput" type="text" placeholder="search here.." />
            <div class="suggestions">
            </div>

        </div>
        <div class="nav">
            <a id="accountlink" onclick="locationchange()"><i class="fas fa-user" style="padding-right: 5px;"></i>Account Info</a>
            <a id="wishlistlink" onclick="locationchange()"><i class="fas fa-heart" style="padding-right: 5px;"></i>Wishlist</a>
            <a id="cartlink" onclick="locationchange()"><i class="fas fa-cart-plus" style="padding-right: 5px;"></i>Cart</a>
            <a href="/TypeTile"><i class="fas fa-home" style="padding-right: 5px;"></i>Home</a>
        </div>
    </div>
    <div class="menu">
        <nav>
            <ul>
                <li><a href="#" style="position: absolute;
                    left: 35%;" onclick="toggleingtogranite()">Granites </a></li>
                <li><a href="#" style="position: absolute;
                    left: 55%;" onclick="toggling()">Marbles </a></li>


            </ul>
        </nav>
    </div>

    <div class="slider">
        <div class="myslider fade">
            <img src="images/first.jpg" style="width: 100%; height: 500px;">
        </div>
        <div class="myslider fade">
            <img src="images/five.jpg" style="width: 100%; height: 500px;">
        </div>
        <div class="myslider fade">
            <img src="images/four.jpg" style="width: 100%; height: 500px;">
        </div>
        <div class="myslider fade">
            <img src="images/Three.jpg" style="width: 100%; height: 500px;">
        </div>
        <div class="myslider fade">
            <img src="images/second.jpg" style="width: 100%; height: 500px;">
        </div>
        <!-- <a class="prev" onclick="plusSlides(-1)">&#10094;</a>
        <a class="next" onclick="plusSlides(1)">&#10095;</a> -->
    </div>

    <br>
    <div style="text-align: center;position: relative; top:50px;">
        <span class="dot"></span>
        <span class="dot"></span>
        <span class="dot"></span>
        <span class="dot"></span>
        <span class="dot"></span>

    </div>

    <!-- Granites code -->
    <div class="granitecontainer">
        <%var i = 1;                                                                                                                       
        granitedata.forEach(function(data){
            var name = data.name.replace(/\s+/g,'')
        %>
            <div class="onegcontainer">
                <div class="info" onclick=location.href="/TypeTile/<%=data.id%>/<%=name%>">
                    <img class='img' style="width: 270px; height: 270px;" src=<%=data.imageurl %> alt="">
                    <h3 id="name" class="name">
                        <%=data.name%>
                    </h3>
                    <p id="location" class="location">Material Origin:

                        <%=data.location%>

                    </p>
                    <!--<br> -->
                    <p id="cost" class="cost">Cost :

                        <%=" "+data.cost+" ₹/sq.feet"%>

                    </p>
                </div>
                <button id="cart" class="cartbtn" onclick="adding('<%=i%>')">Add to Cart</button>
            </div>
            <%i++;})%>
    </div>

    <!-- Marble Code -->
    <div class="marblecontainer">
        <%var i=11;
marbledata.forEach(function(data){
    var name = data.name.replace(/\s+/g,'')
    %>
            <div id="marble" class="onegcontainer">
                <div class="info" onclick=location.href="/TypeTile/marble/<%=data.id%>/<%=name%>">
                    <img class="img" style="width:270px; height:270px;" src=<%=data.imageurl %> >
                    <h3 class="name">
                        <%=data.name%>
                    </h3>
                    <p class="cost">Cost :

                        <%=" " +data.cost+"₹/sq.feet"%>

                    </p>
                </div>
                <button id="cart" class="cartbtn" onclick="adding('<%=i%>')">Add to cart</button>
            </div>
            <%i++;})%>

    </div>
    <div class="container" style="position: fixed;
    top: 590px;
    left: 1250px;" onclick="myorders()">
        <div class="fixed-action-bt">
            <a class="btn-floating red">
                <i style="font-size: 25px; color: white;" class="fas fa-shopping-bag"></i>
            </a>

        </div>



    </div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>


    <script defer src="js/TypeTile.js"></script>
</body>


</html>